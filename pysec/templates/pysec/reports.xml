<?xml version="1.0"?>
<reports form="{{ form }}" report="{{ report }}" quarter="{{ quarter }}">
  {% if error %}
  <error>{{ error }}</error>
  {% endif %}
  {% for c in companies %}
  <report name="{{ c.name }}" cik="{{ c.cik }}" href="{% url 'report-xml' report quarter c.cik %}" />
  {% endfor %}
</reports>

